<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/3.2.0-c" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/3.2.0-c" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 3.2.0-c Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-Assignment">
    <xs:annotation>
      <xs:documentation>Course work assigned to students enrolled in a section.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="AssignmentIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the assignment data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LMSSectionReference" type="EXTENSION-LMSSectionReferenceType">
            <xs:annotation>
              <xs:documentation>The assigment section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Title" type="EXTENSION-Title">
            <xs:annotation>
              <xs:documentation>The assignment title or name.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AssignmentCategory" type="EXTENSION-AssignmentCategory">
            <xs:annotation>
              <xs:documentation>The category or type of assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AssignmentDescription" type="EXTENSION-LMSDescription" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The assignment description.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartDateTime" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The start date and time for the assignment. Students will have access to the assignment after this date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDateTime" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date and time for the assignment. Students will no longer have access to the assignment after this date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DueDateTime" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date and time the assignment is due.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SubmissionType" type="EXTENSION-SubmissionType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The type(s) of submissions available for the assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MaxPoints" type="EXTENSION-Points" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The maximum number of points a student may receive for a submission of the assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-AssignmentSubmission">
    <xs:annotation>
      <xs:documentation>A user's submission of course work for an assignment.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LMSGradeIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the submission.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the user data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserReference" type="EXTENSION-UserReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the user providing the assignment submission.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AssignmentReference" type="EXTENSION-AssignmentReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the submission assignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Status" type="EXTENSION-Status">
            <xs:annotation>
              <xs:documentation>The status of the submission in relation to the late acceptance policy.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SubmissionDateTime" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The date and time of the assignment submission.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EarnedPoints" type="EXTENSION-Points" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The points earned for the submission.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Grade" type="EXTENSION-Grade" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The grade received for the submission.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LMSGrade">
    <xs:annotation>
      <xs:documentation>A grade assigned to a user in a section.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LMSGradeIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the grade.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the grade data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserLMSSectionAssociationReference" type="EXTENSION-UserLMSSectionAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The student section enrollment tied to the grade.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Grade" type="EXTENSION-Grade">
            <xs:annotation>
              <xs:documentation>The user's letter or numeric grade for the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GradeType" type="EXTENSION-GradeType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The type of grade reported. E.g., Current, Final.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-LMSSection">
    <xs:annotation>
      <xs:documentation>An organized grouping of course content and users over a period of time for the purpose of providing instruction.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LMSSectionIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the section data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SISSectionIdentifier" type="EXTENSION-SourceSystemIdentifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The section identifier defined in the Student Information System (SIS).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Title" type="EXTENSION-Title">
            <xs:annotation>
              <xs:documentation>The section title or name.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SectionDescription" type="EXTENSION-LMSDescription" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The section description.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Term" type="EXTENSION-Term" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The enrollment term for the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LMSSectionStatus" type="EXTENSION-Status" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The section status from the source system. E.g., Published, Completed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-User">
    <xs:annotation>
      <xs:documentation>A person using the instructional system.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="UserIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the user.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the user data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserRole" type="EXTENSION-UserRole">
            <xs:annotation>
              <xs:documentation>The role assigned to the user. E.g., Student, Teacher, Administrator.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SISUserIdentifier" type="EXTENSION-SourceSystemIdentifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The user identifier defined in the Student Information System (SIS).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LocalUserIdentifier" type="EXTENSION-SourceSystemIdentifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The user identifier assigned by a school or district.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Name" type="EXTENSION-Name">
            <xs:annotation>
              <xs:documentation>The full name of the user.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EmailAddress" type="EXTENSION-EmailAddress">
            <xs:annotation>
              <xs:documentation>The primary e-mail address for the user.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-UserAttendanceEvent">
    <xs:annotation>
      <xs:documentation>Attendance statuses assigned to users for a specific date.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="UserAttendanceEventIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the user attendance event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the user data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserReference" type="EXTENSION-UserReferenceType">
            <xs:annotation>
              <xs:documentation>A reference to the user associated with the attendance event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserLMSSectionAssociationReference" type="EXTENSION-UserLMSSectionAssociationReferenceType" minOccurs="0">
