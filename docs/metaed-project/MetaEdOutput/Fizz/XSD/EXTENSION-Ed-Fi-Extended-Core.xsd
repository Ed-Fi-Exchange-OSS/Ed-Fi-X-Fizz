              <xs:documentation>A reference to the section enrollment associated with the attendance event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EventDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date of the attendance event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Status" type="EXTENSION-Status">
            <xs:annotation>
              <xs:documentation>A code describing the attendance event. E.g., In Attendance, Excused Absence, Unexcused Absence.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-UserLMSActivity">
