  <xs:annotation>
    <xs:documentation>===== Descriptors =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-UserLMSSectionAssociation">
    <xs:annotation>
      <xs:documentation>The association of a user and section. For a student, this would be a section enrollment. For a teacher, this would be a section assignment.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="UserReference" type="EXTENSION-UserReferenceType">
            <xs:annotation>
              <xs:documentation>A system user associated with the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LMSSectionReference" type="EXTENSION-LMSSectionReferenceType">
            <xs:annotation>
              <xs:documentation>A course section defined in the source system associated with the user.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UserLMSSectionAssociationIdentifier" type="EXTENSION-Identifier">
            <xs:annotation>
              <xs:documentation>A unique numeric identifier assigned to the user section association.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystemIdentifier" type="EXTENSION-SourceSystemIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to a user by the source system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SourceSystem" type="EXTENSION-SourceSystem">
            <xs:annotation>
              <xs:documentation>The system code or name providing the user data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EnrollmentStatus" type="EXTENSION-Status">
            <xs:annotation>
              <xs:documentation>The status of the user section association. E.g., Active, Inactive, Withdrawn.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>Month, day, and year of the user's entry or assignment to the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>Month, day, and year of the user's withdrawal or exit from the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EntityStatus" type="EXTENSION-EntityStatus">
            <xs:annotation>
              <xs:documentation>The status of the record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DeletedAt" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The datetime the record EntityStatus was changed to deleted.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Extended Reference Types =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AssignmentIdentityType">
