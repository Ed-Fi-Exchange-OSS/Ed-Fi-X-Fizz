      <xs:documentation>Identity of an Assignment.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Identity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AssignmentIdentifier" type="EXTENSION-Identifier">
